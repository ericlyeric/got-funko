{
  "name": "got-funko-service",
  "version": "1.0.0",
  "description": "Web app from tracking Game of Throne Vinyl Funko Pops",
  "main": "./service/index.js",
  "scripts": {
    "start": "nodemon ./service/index.js",
    "service": "node ./service/index.js",
    "service-dev": "nodemon ./service/index.js",
    "client": "cd client && npm start",
    "dev": "concurrently --raw \"npm run service-dev\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false cd client && npm install && npm run build"
  },
  "author": "Eric Ly",
  "license": "ISC",
  "dependencies": {
    "async": "^3.2.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "http-errors": "^1.7.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.7",
    "mongoose-unique-validator": "^2.0.3",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "concurrently": "^6.0.0",
    "nodemon": "^2.0.20"
  }
}
